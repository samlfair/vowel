{
	"name": "vowel",
	"version": "0.1.45",
	"homepage": "https://vowel.cc",
	"author": "Sam Littlefair (https://littlefair.ca)",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/samlfair/vowel.git"
	},
	"engines": {
		"node": ">20.0.0"
	},
	"bin": "./bin.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"release": "commit-and-tag-version && git push --follow-tags && npm publish",
		"release:dry": "commit-and-tag-version --dry-run",
		"release:beta": "commit-and-tag-version ---prerelease beta",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"type": "module",
	"dependencies": {
		"@picocss/pico": "^1.5.10",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"any-date-parser": "^1.5.4",
		"change-case": "^5.4.1",
		"commit-and-tag-version": "^12.4.4",
		"fast-xml-parser": "^4.5.0",
		"filter-console": "^1.0.0",
		"js-yaml": "^4.1.0",
		"loading-cli": "^1.1.2",
		"log-update": "^6.0.0",
		"milligram": "^1.4.1",
		"object-path": "^0.11.8",
		"open-props": "^1.6.13",
		"picocolors": "^1.0.1",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"remark": "^15.0.1",
		"remark-admonitions": "^1.2.1",
		"remark-directive": "^3.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-gfm": "^4.0.0",
		"remark-html": "^16.0.1",
		"remark-images": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"spectre.css": "^0.5.9",
		"svelte": "^5.0.4",
		"typescript": "^5.0.0",
		"unified": "^11.0.4",
		"url-metadata": "^3.4.9",
		"vite": "^5.0.12",
		"xml": "^1.0.1"
	}
}